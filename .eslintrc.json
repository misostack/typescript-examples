{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "google",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off",
        "require-jsdoc": "off",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error"        
    }
}
